{
  "name": "stellar-oc-multisig",
  "version": "0.2.0",
  "description": "Implements on-chain signature collection for Stellar",
  "main": "src/index.js",
  "scripts": {
    "test": "jasmine test/*.js 2>&1 | more",
    "get": "git submodule update -i --recursive",
    "format": "standard --fix 'src/**.js'",
    "build-doc": "jsdoc -c jsdoc.json",
    "build-misc": "cp AUTHORS LICENSE README.md package-lock.json web",
    "build-js": "webpack -p",
    "build": "for i in js misc; do npm run build-$i || return; done",
    "check": "npm run get && cp web/package-lock.json . && npm ci && npm run build && cd web && git status",
    "watch": "webpack -d --watch"
  },
  "keywords": [
    "blockchain",
    "stellar",
    "multisig",
    "signature",
    "collection"
  ],
  "homepage": "https://github.com/MisterTicot/js-stellar-oc-multisig",
  "author": "Mister.Ticot",
  "license": "MIT",
  "repository": "github:MisterTicot/js-stellar-oc-multisig",
  "browserList": "last 4 versions, > 0.1%",
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "jasmine": "^3.1.0",
    "jsdoc": "^3.5.5",
    "standard": "^11.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "stellar-sdk": "^0.10.1"
  }
}
