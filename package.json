{
  "name": "@cosmic-plus/oc-multisig",
  "version": "0.3.6",
  "description": "Implements on-chain signature & transaction sharing for Stellar",
  "author": "MisterTicot",
  "repository": "github:cosmic-plus/node-oc-multisig",
  "license": "MIT",
  "browserslist": "last 4 versions, > 0.1%",
  "main": "src/index.js",
  "keywords": [
    "blockchain",
    "stellar",
    "on-chain",
    "multisig",
    "signature",
    "transaction",
    "sharing"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "get": "git submodule update -i --recursive",
    "prettier": "prettier --write --no-semi '**.js' 'src/**.js' '**.json' '**.md'",
    "eslint": "eslint --fix '**.js' 'src/**.js'",
    "lint": "npm run prettier && npm run eslint",
    "build-misc": "cp -f AUTHORS LICENSE README.md package-lock.json web",
    "build-js": "webpack -p",
    "build": "for i in js misc; do npm run build-$i || return; done",
    "rebuild": "npm run get && cp -f web/package-lock.json . && npm ci && npm run build",
    "check": "npm run rebuild && cd web && git status",
    "watch": "webpack -d --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.13.0",
    "prettier": "^1.16.4",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@cosmic-plus/base": "^1.3.3",
    "@cosmic-plus/jsutils": "^1.14.0",
    "@cosmic-plus/loopcall": "^1.0.5"
  }
}
